    "  n <- dim(x)[1]\n",
    "  k <- dim(x)[2]\n",
    "  p <- k + 1\n",
    "  xx <- cbind(rep(1, n), x)\n",
    "  beta <- solve(t(xx) %*% xx) %*% t(xx) %*% y\n",
    "  A <- matrix(lin, ncol = p)\n",
    "  c <- const\n",
    "  t <- sign(A %*% lin - c) * sqrt(gtest(x,y,A,c)$F)  \n",
    "  if(dir == \"+\"){pval = 1 - pt(t,n-p)}\n",
    "  if(dir == \"-\"){pval = pt(t,n-p)}\n",
    "  if(dir == \"b\"){pval = min(2 * (1 - pt(t,n - p)),2 *(pt(t, n - p)))}\n",
    "  return(list(t = t, pvalue = pval))\n",
    "}\n",
    "\n",
    "q <- c(0,0,0,0,0,1)\n",
    "ltest(y, xb, q, 0, \"b\")"
